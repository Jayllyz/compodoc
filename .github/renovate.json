{
    "extends": ["config:base"],
    "ignoreDeps": [
        "os-name",
        "marked",
        "chai",
        "eslint",
        "@typescript-eslint/eslint-plugin",
        "@typescript-eslint/parser",
        "cheerio"
    ],
    "groupName": "all dependencies",
    "separateMajorMinor": false,
    "groupSlug": "all",
    "packageRules": [
        {
            "matchPackagePatterns": ["*"],
            "groupName": "all dependencies",
            "groupSlug": "all"
        }
    ],
    "lockFileMaintenance": {
        "enabled": false
    }
}
